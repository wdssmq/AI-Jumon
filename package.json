{
  "name": "ai-jumon",
  "private": true,
  "version": "1.0.0-beta",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && electron-builder",
    "preview": "vite preview",
    "set-tag": "node -e \"const v=require('./package.json').version;require('child_process').execSync('git tag v'+v);console.log('Created tag: v'+v);\" && git tag",
    "push-tag": "node -e \"const v=require('./package.json').version;require('child_process').execSync('git push origin v'+v);\""
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "dependencies": {
    "markdown-it": "^14.1.0",
    "vue": "^3.5.19",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/markdown-it": "^14.1.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "electron": "^30.5.1",
    "electron-builder": "^24.13.3",
    "js-yaml": "^4.1.0",
    "typescript": "^5.9.2",
    "unocss": "^66.4.2",
    "vite": "^5.4.19",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  },
  "main": "dist-electron/main.js"
}
